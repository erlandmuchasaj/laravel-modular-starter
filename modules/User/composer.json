{
    "name": "modules/user",
    "type": "library",
    "description": "A Generic module for the EMCMS application functionalities",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "license": "proprietary",
    "keywords": [
        "emcms",
        "module"
    ],
    "authors": [
        {
            "name": "Erland Muchasaj",
            "email": "erland.muchasaj@gmail.com",
            "homepage": "https://erlandmuchasaj.tech/",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "erland.muchasaj@gmail.com"
    },
    "require": {
        "modules/core": "^1.0",
        "laravel/sanctum": "^2.14"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Modules\\User\\": "src",
            "Modules\\User\\Database\\Factories\\": "database/factories",
            "Modules\\User\\Database\\Seeders\\": "database/seeders"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\User\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\User\\Providers\\AppServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
