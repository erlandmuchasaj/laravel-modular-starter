{
    "name": "modules/core",
    "type": "library",
    "description": "A Generic module for the application Core functionalities",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "license": "proprietary",
    "keywords": [
        "core",
        "module"
    ],
    "authors": [
        {
            "name": "Erland Muchasaj",
            "email": "erland.muchasaj@gmail.com",
            "homepage": "https://erlandmuchasaj.tech/",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "erland.muchasaj@gmail.com"
    },
    "require": {
        "illuminate/http": "^8|^9|^10",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-honeypot": "^4.3",
        "erusev/parsedown": "^1.7",
        "sentry/sentry-laravel": "^3.2",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Modules\\Core\\": "src/",
            "Modules\\Core\\Database\\Factories\\": "database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Core\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Core\\Providers\\AppServiceProvider"
            ],
            "aliases": {

            }
        }
    }
}
